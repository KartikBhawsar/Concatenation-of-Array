vector<int> getConcatenation(vector<int>& nums) {
        int n=nums.size();
        vector<int>ans(2*n);
       int i=0;
       while(i!=n)
       {
           ans[i]=nums[i];
           i++;
       }
       for(int j=0;j<n;j++)
       {
           ans[i]=nums[j];
           i++;
       }
       return ans;
    }
